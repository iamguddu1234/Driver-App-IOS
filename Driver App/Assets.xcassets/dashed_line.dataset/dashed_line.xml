<!-- dashed_line_square.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <corners android:radius="4dp" />
    <stroke
        android:width="1dp"
        android:color="#33000000"
        android:dashWidth="2dp"
        android:dashGap="4dp" />
    <size
        android:width="100dp"
        android:height="100dp" /> <!-- Set the size of the square as needed -->
</shape>
